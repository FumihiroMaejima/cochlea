{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:css": "stylelint src/**/*.scss",
    "test:unit": "jest --config jest.config.js",
    "prepare": "cd .. && husky install frontend/.husky",
    "create-precommit": "cd .. && husky add frontend/.husky/pre-commit \"cd frontend && yarn lint-staged\"",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "next": "12.1.5",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.1.6",
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.3",
    "jest": "^28.0.3",
    "lint-staged": "^13.1.2",
    "sass": "^1.51.0",
    "stylelint": "^14.8.2",
    "ts-jest": "^28.0.1",
    "typescript": "4.6.4"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.tsx": "eslint --cache --fix",
    "*.js": "eslint --cache --fix",
    "*.jsx": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  }
}
